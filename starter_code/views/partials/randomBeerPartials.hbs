<!-- randomBeerPartials.hbs -->

<div class="randomBeer">
    <img class="randomBeerImg" src="{{this.image_url}}" alt="Card image cap">
    <div class="randomBeer-bottom">
        <h5>{{this.name}}</h5>
        <h4>{{this.tagline}}</h4>
        <p >{{this.description}}</p>
        <h6>Food Pairing</h6>
        <ul>
            <li>{{this.food_pairing.[0]}}</li>
            <li>{{this.food_pairing.[1]}}</li>
            <li>{{this.food_pairing.[2]}}</li>
        </ul>
        <p><span>Brewers Tips:</span> {{this.brewers_tips}}</p>
    </div>
</div>
